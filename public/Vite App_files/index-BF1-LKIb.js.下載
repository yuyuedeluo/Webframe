ves(U,G,te){const pe=[];return this._appendLeaves(pe,U,G=G||10,te=te||0,0),pe}getTile(U,G,te){const pe=this.trees[this._limitZoom(U)],fe=Math.pow(2,U),{extent:xe,radius:Le}=this.options,be=Le/xe,Me=(te-be)/fe,ot=(te+1+be)/fe,Je={features:[]};return this._addTileFeatures(pe.range((G-be)/fe,Me,(G+1+be)/fe,ot),pe.data,G,te,fe,Je),G===0&&this._addTileFeatures(pe.range(1-be/fe,Me,1,ot),pe.data,fe,te,fe,Je),G===fe-1&&this._addTileFeatures(pe.range(0,Me,be/fe,ot),pe.data,-1,te,fe,Je),Je.features.length?Je:null}getClusterExpansionZoom(U){let G=this._getOriginZoom(U)-1;for(;G<=this.options.maxZoom;){const te=this.getChildren(U);if(G++,te.length!==1)break;U=te[0].properties.cluster_id}return G}_appendLeaves(U,G,te,pe,fe){const xe=this.getChildren(G);for(const Le of xe){const be=Le.properties;if(be&&be.cluster?fe+be.point_count<=pe?fe+=be.point_count:fe=this._appendLeaves(U,be.cluster_id,te,pe,fe):fe<pe?fe++:U.push(Le),U.length===te)break}return fe}_createTree(U){const G=new o.c3(U.length/this.stride|0,this.options.nodeSize,Float32Array);for(let te=0;te<U.length;te+=this.stride)G.add(U[te],U[te+1]);return G.finish(),G.data=U,G}_addTileFeatures(U,G,te,pe,fe,xe){for(const Le of U){const be=Le*this.stride,Me=G[be+Ct]>1;let ot,Je,Bt;if(Me)ot=qt(G,be,this.clusterProps),Je=G[be],Bt=G[be+1];else{const $t=this.points[G[be+bt]];ot=$t.properties;const[Ti,Ci]=$t.geometry.coordinates;Je=Lt(Ti),Bt=Mi(Ci)}const si={type:1,geometry:[[Math.round(this.options.extent*(Je*fe-te)),Math.round(this.options.extent*(Bt*fe-pe))]],tags:ot};let ti;ti=Me||this.options.generateId?G[be+bt]:this.points[G[be+bt]].id,ti!==void 0&&(si.id=ti),xe.features.push(si)}}_limitZoom(U){return Math.max(this.options.minZoom,Math.min(Math.floor(+U),this.options.maxZoom+1))}_cluster(U,G){const{radius:te,extent:pe,reduce:fe,minPoints:xe}=this.options,Le=te/(pe*Math.pow(2,G)),be=U.data,Me=[],ot=this.stride;for(let Je=0;Je<be.length;Je+=ot){if(be[Je+2]<=G)continue;be[Je+2]=G;const Bt=be[Je],si=be[Je+1],ti=U.within(be[Je],be[Je+1],Le),$t=be[Je+Ct];let Ti=$t;for(const Ci of ti){const pr=Ci*ot;be[pr+2]>G&&(Ti+=be[pr+Ct])}if(Ti>$t&&Ti>=xe){let Ci,pr=Bt*$t,Kr=si*$t,_n=-1;const an=(Je/ot<<5)+(G+1)+this.points.length;for(const rn of ti){const hr=rn*ot;if(be[hr+2]<=G)continue;be[hr+2]=G;const Ni=be[hr+Ct];pr+=be[hr]*Ni,Kr+=be[hr+1]*Ni,be[hr+4]=an,fe&&(Ci||(Ci=this._map(be,Je,!0),_n=this.clusterProps.length,this.clusterProps.push(Ci)),fe(Ci,this._map(be,hr)))}be[Je+4]=an,Me.push(pr/Ti,Kr/Ti,1/0,an,-1,Ti),fe&&Me.push(_n)}else{for(let Ci=0;Ci<ot;Ci++)Me.push(be[Je+Ci]);if(Ti>1)for(const Ci of ti){const pr=Ci*ot;if(!(be[pr+2]<=G)){be[pr+2]=G;for(let Kr=0;Kr<ot;Kr++)Me.push(be[pr+Kr])}}}}return Me}_getOriginId(U){return U-this.points.length>>5}_getOriginZoom(U){return(U-this.points.length)%32}_map(U,G,te){if(U[G+Ct]>1){const xe=this.clusterProps[U[G+wt]];return te?Object.assign({},xe):xe}const pe=this.points[U[G+bt]].properties,fe=this.options.map(pe);return te&&fe===pe?Object.assign({},fe):fe}}function Jt(Te,U,G){return{type:"Feature",id:Te[U+bt],properties:qt(Te,U,G),geometry:{type:"Point",coordinates:[(te=Te[U],360*(te-.5)),hi(Te[U+1])]}};var te}function qt(Te,U,G){const te=Te[U+Ct],pe=te>=1e4?`${Math.round(te/1e3)}k`:te>=1e3?Math.round(te/100)/10+"k":te,fe=Te[U+wt],xe=fe===-1?{}:Object.assign({},G[fe]);return Object.assign(xe,{cluster:!0,cluster_id:Te[U+bt],point_count:te,point_count_abbreviated:pe})}function Lt(Te){return Te/360+.5}function Mi(Te){const U=Math.sin(Te*Math.PI/180),G=.5-.25*Math.log((1+U)/(1-U))/Math.PI;return G<0?0:G>1?1:G}function hi(Te){const U=(180-360*Te)*Math.PI/180;return 360*Math.atan(Math.exp(U))/Math.PI-90}function _i(Te,U,G,te){let pe=te;const fe=U+(G-U>>1);let xe,Le=G-U;const be=Te[U],Me=Te[U+1],ot=Te[G],Je=Te[G+1];for(let Bt=U+3;Bt<G;Bt+=3){const si=ki(Te[Bt],Te[Bt+1],be,Me,ot,Je);if(si>pe)xe=Bt,pe=si;else if(si===pe){const ti=Math.abs(Bt-fe);ti<Le&&(xe=Bt,Le=ti)}}pe>te&&(xe-U>3&&_i(Te,U,xe,te),Te[xe+2]=pe,G-xe>3&&_i(Te,xe,G,te))}function ki(Te,U,G,te,pe,fe){let xe=pe-G,Le=fe-te;if(xe!==0||Le!==0){const be=((Te-G)*xe+(U-te)*Le)/(xe*xe+Le*Le);be>1?(G=pe,te=fe):be>0&&(G+=xe*be,te+=Le*be)}return xe=Te-G,Le=U-te,xe*xe+Le*Le}function lr(Te,U,G,te){const pe={id:Te??null,type:U,geometry:G,tags:te,minX:1/0,minY:1/0,maxX:-1/0,maxY:-1/0};if(U==="Point"||U==="MultiPoint"||U==="LineString")Cr(pe,G);else if(U==="Polygon")Cr(pe,G[0]);else if(U==="MultiLineString")for(const fe of G)Cr(pe,fe);else if(U==="MultiPolygon")for(const fe of G)Cr(pe,fe[0]);return pe}function Cr(Te,U){for(let G=0;G<U.length;G+=3)Te.minX=Math.min(Te.minX,U[G]),Te.minY=Math.min(Te.minY,U[G+1]),Te.maxX=Math.max(Te.maxX,U[G]),Te.maxY=Math.max(Te.maxY,U[G+1])}function Ar(Te,U,G,te){if(!U.geometry)return;const pe=U.geometry.coordinates;if(pe&&pe.length===0)return;const fe=U.geometry.type,xe=Math.pow(G.tolerance/((1<<G.maxZoom)*G.extent),2);let Le=[],be=U.id;if(G.promoteId?be=U.properties[G.promoteId]:G.generateId&&(be=te||0),fe==="Point")en(pe,Le);else if(fe==="MultiPoint")for(const Me of pe)en(Me,Le);else if(fe==="LineString")Mn(pe,Le,xe,!1);else if(fe==="MultiLineString"){if(G.lineMetrics){for(const Me of pe)Le=[],Mn(Me,Le,xe,!1),Te.push(lr(be,"LineString",Le,U.properties));return}Fn(pe,Le,xe,!1)}else if(fe==="Polygon")Fn(pe,Le,xe,!0);else{if(fe!=="MultiPolygon"){if(fe==="GeometryCollection"){for(const Me of U.geometry.geometries)Ar(Te,{id:be,geometry:Me,properties:U.properties},G,te);return}throw new Error("Input data is not a valid GeoJSON object.")}for(const Me of pe){const ot=[];Fn(Me,ot,xe,!0),Le.push(ot)}}Te.push(lr(be,fe,Le,U.properties))}function en(Te,U){U.push(ls(Te[0]),lo(Te[1]),0)}function Mn(Te,U,G,te){let pe,fe,xe=0;for(let be=0;be<Te.length;be++){const Me=ls(Te[be][0]),ot=lo(Te[be][1]);U.push(Me,ot,0),be>0&&(xe+=te?(pe*ot-Me*fe)/2:Math.sqrt(Math.pow(Me-pe,2)+Math.pow(ot-fe,2))),pe=Me,fe=ot}const Le=U.length-3;U[2]=1,_i(U,0,Le,G),U[Le+2]=1,U.size=Math.abs(xe),U.start=0,U.end=U.size}function Fn(Te,U,G,te){for(let pe=0;pe<Te.length;pe++){const fe=[];Mn(Te[pe],fe,G,te),U.push(fe)}}function ls(Te){return Te/360+.5}function lo(Te){const U=Math.sin(Te*Math.PI/180),G=.5-.25*Math.log((1+U)/(1-U))/Math.PI;return G<0?0:G>1?1:G}function pn(Te,U,G,te,pe,fe,xe,Le){if(te/=U,fe>=(G/=U)&&xe<te)return Te;if(xe<G||fe>=te)return null;const be=[];for(const Me of Te){const ot=Me.geometry;let Je=Me.type;const Bt=pe===0?Me.minX:Me.minY,si=pe===0?Me.maxX:Me.maxY;if(Bt>=G&&si<te){be.push(Me);continue}if(si<G||Bt>=te)continue;let ti=[];if(Je==="Point"||Je==="MultiPoint")kn(ot,ti,G,te,pe);else if(Je==="LineString")cs(ot,ti,G,te,pe,!1,Le.lineMetrics);else if(Je==="MultiLineString")Is(ot,ti,G,te,pe,!1);else if(Je==="Polygon")Is(ot,ti,G,te,pe,!0);else if(Je==="MultiPolygon")for(const $t of ot){const Ti=[];Is($t,Ti,G,te,pe,!0),Ti.length&&ti.push(Ti)}if(ti.length){if(Le.lineMetrics&&Je==="LineString"){for(const $t of ti)be.push(lr(Me.id,Je,$t,Me.tags));continue}Je!=="LineString"&&Je!=="MultiLineString"||(ti.length===1?(Je="LineString",ti=ti[0]):Je="MultiLineString"),Je!=="Point"&&Je!=="MultiPoint"||(Je=ti.length===3?"Point":"MultiPoint"),be.push(lr(Me.id,Je,ti,Me.tags))}}return be.length?be:null}function kn(Te,U,G,te,pe){for(let fe=0;fe<Te.length;fe+=3){const xe=Te[fe+pe];xe>=G&&xe<=te&&uo(U,Te[fe],Te[fe+1],Te[fe+2])}}function cs(Te,U,G,te,pe,fe,xe){let Le=co(Te);const be=pe===0?$r:es;let Me,ot,Je=Te.start;for(let Ti=0;Ti<Te.length-3;Ti+=3){const Ci=Te[Ti],pr=Te[Ti+1],Kr=Te[Ti+2],_n=Te[Ti+3],an=Te[Ti+4],rn=pe===0?Ci:pr,hr=pe===0?_n:an;let Ni=!1;xe&&(Me=Math.sqrt(Math.pow(Ci-_n,2)+Math.pow(pr-an,2))),rn<G?hr>G&&(ot=be(Le,Ci,pr,_n,an,G),xe&&(Le.start=Je+Me*ot)):rn>te?hr<te&&(ot=be(Le,Ci,pr,_n,an,te),xe&&(Le.start=Je+Me*ot)):uo(Le,Ci,pr,Kr),hr<G&&rn>=G&&(ot=be(Le,Ci,pr,_n,an,G),Ni=!0),hr>te&&rn<=te&&(ot=be(Le,Ci,pr,_n,an,te),Ni=!0),!fe&&Ni&&(xe&&(Le.end=Je+Me*ot),U.push(Le),Le=co(Te)),xe&&(Je+=Me)}let Bt=Te.length-3;const si=Te[Bt],ti=Te[Bt+1],$t=pe===0?si:ti;$t>=G&&$t<=te&&uo(Le,si,ti,Te[Bt+2]),Bt=Le.length-3,fe&&Bt>=3&&(Le[Bt]!==Le[0]||Le[Bt+1]!==Le[1])&&uo(Le,Le[0],Le[1],Le[2]),Le.length&&U.push(Le)}function co(Te){const U=[];return U.size=Te.size,U.start=Te.start,U.end=Te.end,U}function Is(Te,U,G,te,pe,fe){for(const xe of Te)cs(xe,U,G,te,pe,fe,!1)}function uo(Te,U,G,te){Te.push(U,G,te)}function $r(Te,U,G,te,pe,fe){const xe=(fe-U)/(te-U);return uo(Te,fe,G+(pe-G)*xe,1),xe}function es(Te,U,G,te,pe,fe){const xe=(fe-G)/(pe-G);return uo(Te,U+(te-U)*xe,fe,1),xe}function $a(Te,U){const G=[];for(let te=0;te<Te.length;te++){const pe=Te[te],fe=pe.type;let xe;if(fe==="Point"||fe==="MultiPoint"||fe==="LineString")xe=ga(pe.geometry,U);else if(fe==="MultiLineString"||fe==="Polygon"){xe=[];for(const Le of pe.geometry)xe.push(ga(Le,U))}else if(fe==="MultiPolygon"){xe=[];for(const Le of pe.geometry){const be=[];for(const Me of Le)be.push(ga(Me,U));xe.push(be)}}G.push(lr(pe.id,fe,xe,pe.tags))}return G}function ga(Te,U){const G=[];G.size=Te.size,Te.start!==void 0&&(G.start=Te.start,G.end=Te.end);for(let te=0;te<Te.length;te+=3)G.push(Te[te]+U,Te[te+1],Te[te+2]);return G}function Us(Te,U){if(Te.transformed)return Te;const G=1<<Te.z,te=Te.x,pe=Te.y;for(const fe of Te.features){const xe=fe.geometry,Le=fe.type;if(fe.geometry=[],Le===1)for(let be=0;be<xe.length;be+=2)fe.geometry.push(Fu(xe[be],xe[be+1],U,G,te,pe));else for(let be=0;be<xe.length;be++){const Me=[];for(let ot=0;ot<xe[be].length;ot+=2)Me.push(Fu(xe[be][ot],xe[be][ot+1],U,G,te,pe));fe.geometry.push(Me)}}return Te.transformed=!0,Te}function Fu(Te,U,G,te,pe,fe){return[Math.round(G*(Te*te-pe)),Math.round(G*(U*te-fe))]}function Sc(Te,U,G,te,pe){const fe=U===pe.maxZoom?0:pe.tolerance/((1<<U)*pe.extent),xe={features:[],numPoints:0,numSimplified:0,numFeatures:Te.length,source:null,x:G,y:te,z:U,transformed:!1,minX:2,minY:1,maxX:-1,maxY:0};for(const Le of Te)Pd(xe,Le,fe,pe);return xe}function Pd(Te,U,G,te){const pe=U.geometry,fe=U.type,xe=[];if(Te.minX=Math.min(Te.minX,U.minX),Te.minY=Math.min(Te.minY,U.minY),Te.maxX=Math.max(Te.maxX,U.maxX),Te.maxY=Math.max(Te.maxY,U.maxY),fe==="P